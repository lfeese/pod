flush ruleset;

table ip loramon {
        chain forward {
                type filter hook forward priority mangle; policy accept;
        }

        chain postrouting {
                type nat hook postrouting priority srcnat;
                
                oifname != "wifi-ap" ip saddr == 10.60.0.1/24 ip daddr != 10.60.0.1/24 masquerade;
        }
}
